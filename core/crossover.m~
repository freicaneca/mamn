function [filho1, filho2] = crossover(pai1, pai2, nPoints)

    [~,c1] = size(pai1);
    [~,c2] = size(pai2);
    
    


    if nPoints > c1
        error('Maior número de cortes do que a quantidade de cl');
    end

end